{
  "name": "optimus",
  "version": "0.0.0",
  "description": "Transformer service for user repositories",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "doc": "jsdoc index.js lib/ -d doc/; open -a 'Google Chrome' doc/index.html",
    "lint": "jshint index.js lib/",
    "lab": "lab -v -c -m 10000 -a code",
    "circle-lab": "lab -v -c -m 10000 -a code test/unit/",
    "test": "npm run lint && npm run lab",
    "circle-test": "npm run lint && npm run circle-lab"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeNow/optimus.git"
  },
  "jshintConfig": {
    "node": true,
    "curly": true
  },
  "keywords": [
    "filesystem",
    "transformation",
    "service"
  ],
  "author": "Ryan Sandor Richards <ryan@runnable.com> (http://www.runnable.com)",
  "bugs": {
    "url": "https://github.com/CodeNow/optimus/issues"
  },
  "homepage": "https://github.com/CodeNow/optimus",
  "devDependencies": {
    "app-root-path": "^1.0.0",
    "callback-count": "^0.1.0",
    "code": "^1.4.0",
    "jsdoc": "^3.3.0-beta3",
    "jshint": "^2.7.0",
    "lab": "^5.5.1",
    "request": "^2.55.0",
    "rewire": "^2.3.3",
    "sinon": "^1.14.1"
  },
  "dependencies": {
    "101": "^0.16.1",
    "async": "^0.9.0",
    "aws-sdk": "^2.1.26",
    "body-parser": "^1.12.3",
    "cluster-man": "^1.1.0",
    "connect-datadog": "0.0.5",
    "debug": "^2.1.3",
    "express": "^4.12.3",
    "express-boom": "^0.3.0",
    "fs-transform": "^3.0.3",
    "loadenv": "^1.0.3",
    "monitor-dog": "^1.1.2",
    "simple-api-client": "^0.6.0"
  }
}
